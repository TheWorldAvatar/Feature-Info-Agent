PREFIX credo: <http://theworldavatar.com/ontology/ontocredo/ontocredo.owl#>
select ?propertyLabel ?value
{ SERVICE [ONTOP] {[IRI] ?hasProperty ?property. ?property ?hasValue ?value.}
