{
    "database": "postgres",
    "workspace": "twa",
    "datasetDirectory": "sample-data",
    "skip": false,
    "dataSubsets": [
        {
            "type": "tboxcsv",
            "subdirectory": "tboxes",
            "skip": false
        },
        {
            "type": "vector",
            "skip": false,
            "schema": "public",
            "table": "castles",
            "subdirectory": "aboxes",
            "ogr2ogrOptions": {
                "sridIn": "EPSG:4326",
                "sridOut": "EPSG:4326",
                "inputDatasetOpenOptions": {
                    "X_POSSIBLE_NAMES": "Longitude",
                    "Y_POSSIBLE_NAMES": "Latitude",
                    "EMPTY_STRING_AS_NULL": "YES"
                }
            },
            "geoServerSettings": {
                "virtualTable": {
                    "name": "castles_geo",
                    "sql": "SELECT wkb_geometry, 'http://FIATESTSTACK-blazegraph:8080/blazegraph/namespace/castles/sparql' as endpoint, \"Castle Name\" as name,  \"IRI\" as iri FROM castles",
                    "escapeSql": false,
                    "geometry": {
                        "name": "wkb_geometry",
                        "type": "Point",
                        "srid": 4326
                    }
                }
            }
        }
    ]
}